<body>

    <!-- ======= Header ======= -->
    <header id="header" class="header fixed-top d-flex align-items-center">

        <div class="d-flex align-items-center justify-content-between">
            <a asp-controller="Home" asp-action="Index" class="logo d-flex align-items-center">
                <img src="~/img/logo.png" alt="">
                <span class="d-none d-lg-block">Termodinamik</span>
            </a>
            <i class="bi bi-list toggle-sidebar-btn"></i>
        </div><!-- End Logo -->

    </header><!-- End Header -->
    <!-- ======= Sidebar ======= -->
    <aside id="sidebar" class="sidebar">

        <ul class="sidebar-nav" id="sidebar-nav">
            <li class="nav-item">
                <a class="nav-link collapsed" data-bs-target="#calculations-nav" data-bs-toggle="collapse" href="#">
                    <i class="bi bi-calculator"></i><span>Hesaplamalar</span><i class="bi bi-chevron-down ms-auto"></i>
                </a>
                <ul id="calculations-nav" class="nav-content collapse " data-bs-parent="#sidebar-nav">
                    <li>
                        <a asp-controller="Calculation" asp-action="SaturatedWaterTemperature">
                            <i class="bi bi-circle"></i><span>Doymuş Su - Sıcaklık Hesapları</span>
                        </a>
                    </li>
                    <li>
                        <a asp-controller="Calculation" asp-action="SaturatedWaterPressure">
                            <i class="bi bi-circle"></i><span>Doymuş Su - Basınç Hesapları</span>
                        </a>
                    </li>
                    <li>
                        <a asp-controller="Calculation" asp-action="SaturatedR134ATemperature">
                            <i class="bi bi-circle"></i><span>R134a - Sıcaklık Hesapları</span>
                        </a>
                    </li>
                    <li>
                        <a asp-controller="Calculation" asp-action="SaturatedR134APressure">
                            <i class="bi bi-circle"></i><span>R134a - Basınç Hesapları</span>
                        </a>
                    </li>
                </ul>
            </li><!-- End Calculations Nav -->
        </ul>
        <ul class="sidebar-nav" id="sidebar-nav">
            <li class="nav-item">
                <a class="nav-link collapsed" data-bs-target="#tables-nav" data-bs-toggle="collapse" href="#">
                    <i class="bi bi-layout-text-window-reverse"></i><span>Tablolar</span><i class="bi bi-chevron-down ms-auto"></i>
                </a>
                <ul id="tables-nav" class="nav-content collapse " data-bs-parent="#sidebar-nav">
                    <li>
                        <a asp-controller="Table" asp-action="SaturatedWaterTemperature">
                            <i class="bi bi-circle"></i><span>Doymuş Su - Sıcaklık Tabloları</span>
                        </a>
                    </li>
                    <li>
                        <a asp-controller="Table" asp-action="SaturatedWaterPressure">
                            <i class="bi bi-circle"></i><span>Doymuş Su - Basınç Tabloları</span>
                        </a>
                    </li>
                    <li>
                        <a asp-controller="Table" asp-action="SaturatedR134ATemperature">
                            <i class="bi bi-circle"></i><span>R134a - Sıcaklık Tabloları</span>
                        </a>
                    </li>
                    <li>
                        <a asp-controller="Table" asp-action="SaturatedR134APressure">
                            <i class="bi bi-circle"></i><span>R134a - Basınç Tabloları</span>
                        </a>
                    </li>
                </ul>
            </li><!-- End Tables Nav -->
        </ul>

    </aside><!-- End Sidebar-->

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <!-- ======= Footer ======= -->
    <footer id="footer" class="footer">
        <div class="copyright">
            &copy;  <strong><span>Termodinamik Hesap Makinesi</span></strong>.
        </div>
        <div class="credits">
            <!-- All the links in the footer should remain intact. -->
